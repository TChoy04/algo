#include <bits/stdc++.h>
using namespace std;
 
 
int main(){
	ios::sync_with_stdio(0);
	cin.tie(0);
	int tc;
	cin>>tc;
	long long res = INT_MIN;
	long long temp = 0;
	while(tc--){
		long long val = 0;
		cin>>val;
		temp = max(val,temp+val);
		res = max(temp,res);
	}
	cout<<res;
	return 0;
}
