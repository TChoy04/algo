#include <bits/stdc++.h>
using namespace std;

void setIO(string s) {
    freopen((s + ".in").c_str(), "r", stdin);
    freopen((s + ".out").c_str(), "w", stdout);
}

/*
y/x = k^n
y = k^n * x
L2<= k^n*x <= R2
L2/k^n <= x <= R2/k^n
*/
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int tc;
    cin >> tc;

    while (tc--) {
        string curr;
        cin>>curr;
        for(int i = 0; i<curr.size()-2; i++){
        	cout<<curr[i];
        }
        cout<<"i\n";
    }

    return 0;
}
